{% extends "LiipHelloBundle::vie.html.twig" %}

{% block content %}

        <div about="http://symfony-standard.lo/liip" rel="dcterms:hasPart" rev="dcterms:partOf">
            <article typeof="sioc:Post" about="http://symfony-standard.lo/liip/vie">
                <h1 property="dcterms:title">{{ article.title }}</h1>
                <div property="sioc:content">
                    {{ article.body|raw }}
                </div>
           </article>
        </div>

{% endblock content %}
